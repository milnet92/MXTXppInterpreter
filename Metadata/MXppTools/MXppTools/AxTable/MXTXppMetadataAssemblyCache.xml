		<Methods>
			<Method>
				<Name>namespaceExists</Name>
				<Source><![CDATA[
    public static boolean namespaceExists(MXTAssemblyNamespace _namespace, boolean _partialSearch)
    {
        boolean ret = (select firstonly RecId from MXTXppMetadataAssemblyCache
                    index NamespaceIdx
                    where MXTXppMetadataAssemblyCache.AssemblyNamespace == _namespace).RecId != 0;

        if (!ret && _partialSearch)
        {
            str searchNamespace = _namespace + '.*';

            return (select firstonly RecId from MXTXppMetadataAssemblyCache
                index NamespaceIdx
                where MXTXppMetadataAssemblyCache.AssemblyNamespace like searchNamespace).RecId != 0;
        }

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>typeExists</Name>
				<Source><![CDATA[
    public static boolean typeExists(MXTAssemblyNamespace _namespace, MXTAssemblyTypeName _typeName)
    {
        return (select firstonly RecId from MXTXppMetadataAssemblyCache
                index TypeIdx
                where MXTXppMetadataAssemblyCache.AssemblyNamespace == _namespace &&
                      MXTXppMetadataAssemblyCache.TypeName          == _typeName).RecId != 0;
    }

]]></Source>
			</Method>
			<Method>
				<Name>getAssemblyName</Name>
				<Source><![CDATA[
    public static MXTAssemblyDisplayName getAssemblyName(MXTAssemblyNamespace _namespace, MXTAssemblyTypeName _typeName)
    {
        return (select firstonly AssemblyDisplayName from MXTXppMetadataAssemblyCache
                index TypeIdx
                where MXTXppMetadataAssemblyCache.AssemblyNamespace == _namespace &&
                      MXTXppMetadataAssemblyCache.TypeName          == _typeName).AssemblyDisplayName;
    }

]]></Source>
			</Method>
