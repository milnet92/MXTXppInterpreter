						<Source><![CDATA[
        /// <summary>
        ///
        /// </summary>
        public void write()
        {
            if (!element.closedOk())
            {
                return;
            }

            ttsbegin;

            if (!currentScript)
            {
                currentScript = MXTXppInterpreterScript::createService(MXTXppInterpreterService.ServiceId, ScriptDescription.text());
            }

            MXTXppInterpreterService.Discoverable = NoYes::Yes;
            MXTXppInterpreterService.Script = currentScript.RecId;
            MXTXppInterpreterService.insert();

            ttscommit;

            // Update service parameters based on the current script source code
            MXTXppInterpreterServiceParameters::updateServiceParameters(MXTXppInterpreterService);
        }

]]></Source>
