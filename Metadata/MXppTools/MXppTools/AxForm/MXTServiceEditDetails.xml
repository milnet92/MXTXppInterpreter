				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void close()
    {
        if (fromEditor)
        {
            element.args().record(MXTXppInterpreterService);
        }
        else if (MXTXppInterpreterService)
        {
            MenuFunction menuFunc = new MenuFunction(menuItemDisplayStr(MXTScriptEditor), MenuItemType::Display);
            Args args = new Args();
            args.record(MXTXppInterpreterService);
            args.openMode(OpenMode::Edit);

            menuFunc.run(args);
        }

        super();
    }

