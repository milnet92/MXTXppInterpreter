			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public void init()
    {
        switch (this.args().dataset())
        {
            case tableNum(MXTXppInterpreterService):
                callerService = this.args().record();
                break;
            case tableNum(MXTXppInterpreterScript):
                MXTXppInterpreterScript script = this.args().record();
                callerService = MXTXppInterpreterService::findByScript(script.RecId);
                break;
            default:
                throw error(error::wrongUseOfFunction(funcName()));
        }

        super();

        this.design().caption(strFmt("@MXT:ServiceParametersCaption", callerService.ServiceId));
    }

]]></Source>
